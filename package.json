{
  "name": "json-key-suggestion",
  "displayName": "json-key-suggestion",
  "description": "JSON key auto complete extension",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:mdx"
  ],
  "contributes": {
    "languages": [
      {
          "id": "mdx",
          "aliases": ["MDX", "mdx"],
          "extensions": [".mdx"]
      }
    ]
  },
  "main": "./extension.js",
  "contributes": {
    "configuration": {
        "title": "JSON Key Suggestion Settings",
        "properties": {
            "jsonKeySuggestion.jsonFilePath": {
                "type": "string",
                "default": "",
                "description": "JSON 파일 경로를 설정합니다. 절대 경로나 워크스페이스 상대 경로를 사용할 수 있습니다."
            }
        }
    },
    "commands": [{
      "command": "json-key-suggestion.helloWorld",
      "title": "Hello World"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.16.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
